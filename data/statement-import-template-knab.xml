<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">
	<record id="knab_csv_import" model="account.statement.import.sheet.mapping">
		<field name="name">KNAB CSV import</field>
		<field name="float_thousands_sep">dot</field>
		<field name="float_decimal_sep">comma</field>
		<field name="file_encoding">utf-8</field>
		<field name="delimiter">semicolon</field>
		<field name="quotechar">"</field>
		<field name="timestamp_format">%d-%m-%Y</field>
		<field name="timestamp_column">Transactiedatum</field>
		<field name="amount_column">Bedrag</field>
		<field name="debit_credit_column">CreditDebet</field>
		<field name="debit_value">D</field>
		<field name="credit_value">C</field>
		<field name="transaction_id_column">Referentie</field>
		<field name="description_column">Omschrijving</field>
		<field name="partner_name_column">Tegenrekeninghouder</field>
		<field name="bank_account_column">Tegenrekeningnummer</field>
		<field name="display_name">KNAB CSV import</field>
	</record>
</odoo>
